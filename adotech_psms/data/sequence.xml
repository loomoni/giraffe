<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!--Adding New Sequence For A Field-->

    <data noupdate="1">
        <!-- Sequences for grand budget -->
        <record id="seq_budget" model="ir.sequence">
            <field name="name">Budget Sequence</field>
            <field name="code">budget.sequence</field>
            <field name="prefix">EGBG</field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
